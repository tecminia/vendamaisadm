  "screenshots": [
    {
      "src": "./screenshot-login.png",
      "sizes": "375x812",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Tela de Login"
    },
    {
      "src": "./screenshot-dashboard.png",
      "sizes": "375x812",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Dashboard Administrativo"
    },
    {
      "src": "./screenshot-orders.png",
      "sizes": "375x812",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Gerenciamento de Pedidos"
    }
  ],
  
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Acessar o painel principal",
      "url": "./painelvendas.html",
      "icons": [
        {
          "src": "./shortcut-dashboard.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Novo Pedido",
      "short_name": "Pedido",
      "description": "Visualizar novo pedido",
      "url": "./painelvendas.html?screen=orders",
      "icons": [
        {
          "src": "./shortcut-order.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Vitrine",
      "short_name": "Vitrine",
      "description": "Ver loja virtual",
      "url": "./vitrine.html",
      "icons": [
        {
          "src": "./shortcut-store.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Configurações",
      "short_name": "Config",
      "description": "Configurar loja",
      "url": "./painelvendas.html?screen=config",
      "icons": [
        {
          "src": "./shortcut-settings.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  
  "related_applications": [],
  "prefer_related_applications": false,
  
  "protocol_handlers": [
    {
      "protocol": "web+vendaplus",
      "url": "/?store=%s"
    }
  ],
  
  "display_override": [
    "window-controls-overlay",
    "minimal-ui"
  ],
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  
  "handle_links": "preferred",
  
  "share_target": {
    "action": "./painelvendas.html?share",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  
  "file_handlers": [
    {
      "action": "./painelvendas.html?upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  
  "permissions": [
    "clipboard-write",
    "clipboard-read",
    "notifications",
    "geolocation"
  ]
}